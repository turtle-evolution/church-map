{
  "name": "react-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@elastic/eui": "^34.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "apollo-link": "^1.2.14",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "axios": "^0.21.1",
    "gh-pages": "^4.0.0",
    "graphql": "^16.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.52.3",
    "subscriptions-transport-ws": "^0.11.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage  --watchAll=false --verbose=true",
    "test:watch": "npm test -- --watchAll=true --verbose=true",
    "view:coverage": "open coverage/lcov-report/index.html",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:push": "cross-env CI=true yarn test --noStackTrace --coverage=false --watchAll=false --verbose=false --testTimeout=20000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test --watchAll=false --silent && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pretty-quick --staged",
      "prettier --write",
      "eslint --cache --fix",
      "eslint --fix --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "((\\.| /)(stories|story|mock))\\.[jt]sx?$",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/helper/*",
      "<rootDir>/src/api/*",
      "<rootDir>/src/mock/*"
    ]
  },
  "devDependencies": {
    "@elastic/datemath": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.2"
  },
  "peerDependencies": {},
  "homepage": "./*"
}
